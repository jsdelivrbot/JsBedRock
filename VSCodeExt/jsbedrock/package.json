{
	"name": "jsbedrock",
	"displayName": "JsBedRock",
	"description": "VSCode Extensions for javascript development using JsBedRock.",
	"version": "0.0.1",
	"publisher": "JsBedRock",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./extension",
	"contributes": {
		"commands": [{
			"command": "extension.compile",
			"title": "JsBedRock Compile"
		},{
			"command": "extension.about",
			"title": "JsBedRock About"
		}],
		"keybindings": [{
			"command": "extension.compile",
			"key": "ctrl+f1",
			"mac": "cmd+f1",
			"when": "editorTextFocus"
		}]
	},
	"dependencies": {
		"npm": "3.5.2",
		"uglify-js": "2.6.1",
		"fstream": "1.0.8",
		"tar": "2.2.1",
		"jshint": "2.9.1-rc1"
	},
	"devDependencies": {
		"vscode": "0.10.x"
	}
}